@article{Danos2004formala,
	series = {Computational {Systems} {Biology}},
	title = {Formal molecular biology},
	volume = {325},
	issn = {0304-3975},
	url = {http://www.sciencedirect.com/science/article/pii/S0304397504002336},
	doi = {10.1016/j.tcs.2004.03.065},
	abstract = {A language of formal proteins, the$\kappa$-calculus, is introduced. Interactions are modeled at the domain level, bonds are represented by means of shared names, and reactions are required to satisfy a causality requirement of monotonicity. An example of a simplified signalling pathway is introduced to illustrate how standard biological events can be expressed in our protein language. A more comprehensive example, the lactose operon, is also developed, bringing some confidence in the formalism considered as a modeling language. Then a finer-grained concurrent model, the m$\kappa$-calculus, is considered, where interactions have to be at most binary. We show how to embed the coarser-grained language in the latter, a property which we call self-assembly. Finally we show how the finer-grained language can itself be encoded in $\pi$-calculus, a standard foundational language for concurrency theory.},
	number = {1},
	urldate = {2019-04-02},
	journal = {Theoretical Computer Science},
	author = {Danos, Vincent and Laneve, Cosimo},
	month = sep,
	year = {2004},
	note = {00516},

  journal = {Frontiers in Computational Neuroscience},
  keywords = {c,complex systems simulation,elegans,emergent behavior,integrative modeling,integrative modeling, C. elegans, emergent behavior, complex systems simulation, open science,open science},
  month = nov,
  pages = {1--7},
  pmid = {25404913},
  title = {{OpenWorm: an open-science approach to modeling Caenorhabditis elegans}},
  url = {http://journal.frontiersin.org/article/10.3389/fncom.2014.00137/abstract},
  volume = {8},

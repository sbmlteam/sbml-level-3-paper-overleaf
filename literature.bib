@article{Glont2018biomodels,
	title = {{BioModels}: expanding horizons to include more modelling approaches and formats},
	volume = {46},
	issn = {0305-1048},
	url = {http://dx.doi.org/10.1093/nar/gkx1023},
	doi = {10.1093/nar/gkx1023},
	abstract = {BioModels serves as a central repository of mathematical models
representing biological processes. It offers a platform to make
mathematical models easily shareable across the systems modelling
community, thereby supporting model reuse. To facilitate hosting a broader
range of model formats derived from diverse modelling approaches and
tools, a new infrastructure for BioModels has been developed that is
available at http://www.ebi.ac.uk/biomodels. This new system allows
submitting and sharing of a wide range of models with improved support for
formats other than SBML. It also offers a version-control backed
environment in which authors and curators can work collaboratively to
curate models. This article summarises the features available in the
current system and discusses the potential benefit they offer to the users
over the previous system. In summary, the new portal broadens the scope of
models accepted in BioModels and supports collaborative model curation
which is crucial for model reproducibility and sharing.},
